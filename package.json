{
  "name": "lab",
  "private": true,
  "packageManager": "pnpm@10.28.1",
  "description": "Experimental prototypes.",
  "author": "sabertazimi <sabertazimi@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/sabertazimi/lab",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sabertazimi/lab.git"
  },
  "bugs": {
    "url": "https://github.com/sabertazimi/lab/issues"
  },
  "keywords": [
    "html",
    "css",
    "javascript",
    "react",
    "vue",
    "nodejs",
    "front-end",
    "web",
    "rust",
    "cargo",
    "python",
    "uv",
    "ai",
    "llm",
    "agent",
    "claude"
  ],
  "scripts": {
    "web:build": "pnpm -r run build && node scripts/post-build.js && cp -fr vercel.json dist/",
    "web:lint": "pnpm -r run lint",
    "web:lint:fix": "pnpm -r run lint:fix",
    "web:release": "lerna version --force-publish --sign-git-commit --sign-git-tag",
    "web:dev": "pnpm web:dev:portfolio",
    "web:dev:portfolio": "pnpm --filter @lab/portfolio dev",
    "web:dev:m-league-reviewer": "pnpm --filter @lab/m-league-reviewer dev",
    "web:dev:mortal-ui": "pnpm --filter @lab/mortal-ui dev",
    "web:dev:echarts-dashboard": "pnpm --filter @lab/echarts-dashboard dev",
    "web:dev:react-renderer": "pnpm --filter @lab/react-renderer dev",
    "web:dev:vue-design": "pnpm --filter @lab/vue-design dev",
    "web:dev:vue-trello": "pnpm --filter @lab/vue-trello dev",
    "web:test": "pnpm -r run test",
    "rust:build": "cargo build --release",
    "rust:deps": "cargo install mdbook cargo-edit cargo-release cargo-workspace",
    "rust:dev:basis": "cargo run -p basis",
    "rust:dev:rustlings": "cd crates/rustlings && cargo run -- watch",
    "rust:docs": "pnpm rust:docs:book && pnpm rust:docs:api",
    "rust:docs:book": "mdbook build",
    "rust:docs:api": "cargo doc --workspace --no-deps && mv target/doc book/",
    "rust:e2e": "pnpm rust:e2e:basis && pnpm rust:e2e:rustlings",
    "rust:e2e:basis": "cargo run -p basis --release",
    "rust:e2e:rustlings": "cd crates/rustlings && cargo run --release -- verify",
    "rust:format": "cargo fmt && cargo clippy --fix --allow-dirty --allow-staged",
    "rust:lint": "cargo fmt --check && cargo clippy --all-targets --release --locked -- -D clippy::all",
    "rust:prelease": "cargo release",
    "rust:release": "cargo release -x",
    "rust:start": "pnpm rust:dev:rustlings",
    "rust:test": "cargo test"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^6.1.0",
    "@dg-scripts/stylelint-config": "^6.1.0",
    "@types/node": "^24.10.9",
    "eslint": "^9.39.2",
    "lerna": "^9.0.3",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "stylelint": "^17.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
