{
  "name": "@awesome-web/vue-basis",
  "version": "1.9.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build && pnpm build:post",
    "build:post": "mkdir -p ../../build/vue-basis && cp -fr ./dist/* ../../build/vue-basis",
    "dev": "vite",
    "lint": "pnpm lint:style && pnpm lint:type-check",
    "lint:style": "eslint ./src && stylelint ./src/**/*.{css,vue}",
    "lint:type-check": "vue-tsc --noEmit",
    "lint:fix": "eslint --fix ./src && stylelint ./src/**/*.{css,vue} --fix",
    "serve": "vite preview"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.21.4",
    "@dg-scripts/stylelint-config": "^5.21.4",
    "@types/node": "^22.10.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "stylelint": "^16.11.0",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2",
    "vite": "^6.0.1",
    "vue-tsc": "^2.1.10"
  }
}
