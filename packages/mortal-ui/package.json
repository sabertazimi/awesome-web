{
  "name": "mortal-ui",
  "type": "module",
  "version": "1.9.0",
  "private": true,
  "packageManager": "pnpm@10.23.0",
  "scripts": {
    "start": "pnpm dev",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" -- && pnpm build:post",
    "build:post": "mkdirp ../../build/mortal-ui && cp -fr ./dist/* ../../build/mortal-ui && cp -fr ./e2e ../../build/mortal-ui",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint ./src",
    "lint:fix": "pnpm format",
    "format": "eslint ./src --fix"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@unhead/vue": "^2.0.19",
    "element-plus": "^2.11.8",
    "pinia": "^3.0.4",
    "vue": "^3.5.24",
    "vue-i18n": "^11.1.12"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.21.7",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.13",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.0.2",
    "jsdom": "^27.2.0",
    "npm-run-all2": "^8.0.4",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.4",
    "vitest": "^4.0.13",
    "vue-tsc": "^3.1.4"
  }
}
