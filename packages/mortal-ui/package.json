{
  "name": "mortal-ui",
  "type": "module",
  "version": "2.4.0",
  "private": true,
  "packageManager": "pnpm@10.26.1",
  "scripts": {
    "start": "pnpm dev",
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" -- && pnpm build:post",
    "build:post": "mkdirp ../../dist/mortal-ui && cp -fr ./dist/* ../../dist/mortal-ui && cp -fr ./e2e ../../dist/mortal-ui",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint ./src",
    "lint:fix": "pnpm format",
    "format": "eslint ./src --fix"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@unhead/vue": "^2.0.19",
    "element-plus": "^2.13.0",
    "pinia": "^3.0.4",
    "vue": "^3.5.26",
    "vue-i18n": "^11.2.2"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^6.0.0",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-format": "^1.1.0",
    "jsdom": "^27.3.0",
    "npm-run-all2": "^8.0.4",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.1.8"
  }
}
