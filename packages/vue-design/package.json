{
  "name": "@awesome-web/vue-design",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc && vite build && pnpm build:post",
    "build:post": "mkdirp ../../dist/vue-design && cp -fr ./dist/* ../../dist/vue-design",
    "dev": "vite",
    "lint": "pnpm lint:style && pnpm lint:type-check",
    "lint:fix": "eslint ./src --fix",
    "lint:style": "eslint ./src",
    "lint:type-check": "tsc --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "snabbdom": "^3.6.3"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.21.8",
    "eslint": "^9.39.1",
    "prettier": "^3.7.3",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  }
}
