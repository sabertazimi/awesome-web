{
  "name": "m-league-reviewer",
  "type": "module",
  "version": "1.9.0",
  "private": true,
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "start": "pnpm run dev",
    "dev": "vite",
    "build": "tsc -b && vite build && pnpm build:post",
    "build:post": "mkdirp ../../dist/m-league-reviewer && cp -fr ./dist/* ../../dist/m-league-reviewer",
    "lint": "eslint .",
    "test": "vitest run --coverage",
    "preview": "vite preview",
    "fetch": "tsc --project tsconfig.scripts.json && node scripts/fetch.js && node scripts/fix.js",
    "fetch:game-schedule": "tsc --project tsconfig.scripts.json && node scripts/fetch-game-schedule.js"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^13.8.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/vite": "^4.1.17",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-day-picker": "^9.11.3",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.6",
    "react-use": "^17.6.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@dg-scripts/eslint-config": "^5.21.7",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.15",
    "axios": "^1.13.2",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "eslint": "^9.39.1",
    "node-html-parser": "^7.0.1",
    "prettier": "^3.7.3",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "shadcn": "^3.5.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.15"
  }
}
