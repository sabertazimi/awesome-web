{
  "name": "echarts-dashboard",
  "type": "module",
  "version": "1.9.0",
  "private": true,
  "packageManager": "pnpm@10.18.2",
  "scripts": {
    "start": "pnpm run dev",
    "dev": "vite",
    "build": "vite build && pnpm build:post",
    "build:post": "mkdir -p ../../build/echarts-dashboard && cp -fr ./dist/* ../../build/echarts-dashboard",
    "preview": "vite preview",
    "lint": "vue-tsc -b"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^22.18.10",
    "@vueuse/core": "^14.0.0",
    "echarts": "^6.0.0",
    "echarts-wordcloud": "^2.1.0",
    "tailwindcss": "^4.1.14",
    "vue": "^3.5.22",
    "vue-echarts": "^8.0.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.1.9",
    "vue-tsc": "^3.1.1"
  }
}
