{
  "name": "echarts-dashboard",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "start": "pnpm run dev",
    "dev": "vite",
    "build": "vite build && pnpm build:post",
    "build:post": "mkdir -p ../../build/echarts-dashboard && cp -fr ./dist/* ../../build/echarts-dashboard",
    "preview": "vite preview",
    "lint": "vue-tsc -b"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^22.16.0",
    "@vueuse/core": "^13.5.0",
    "echarts": "^5.6.0",
    "echarts-wordcloud": "^2.1.0",
    "tailwindcss": "^4.1.11",
    "vue": "^3.5.17",
    "vue-echarts": "^7.0.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.0",
    "vue-tsc": "^2.2.12"
  }
}
