{
  "name": "echarts-dashboard",
  "type": "module",
  "version": "1.9.0",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "scripts": {
    "start": "pnpm run dev",
    "dev": "vite",
    "build": "vite build && pnpm build:post",
    "build:post": "mkdir -p ../../build/echarts-dashboard && cp -fr ./dist/* ../../build/echarts-dashboard",
    "preview": "vite preview",
    "lint": "vue-tsc -b"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^22.18.6",
    "@vueuse/core": "^13.9.0",
    "echarts": "^6.0.0",
    "echarts-wordcloud": "^2.1.0",
    "tailwindcss": "^4.1.13",
    "vue": "^3.5.22",
    "vue-echarts": "^7.0.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.2",
    "vite": "^7.1.7",
    "vue-tsc": "^3.0.8"
  }
}
