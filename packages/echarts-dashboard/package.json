{
  "name": "echarts-dashboard",
  "type": "module",
  "version": "2.3.0",
  "private": true,
  "packageManager": "pnpm@10.26.1",
  "scripts": {
    "start": "pnpm run dev",
    "dev": "vite",
    "build": "vite build && pnpm build:post",
    "build:post": "mkdirp ../../dist/echarts-dashboard && cp -fr ./dist/* ../../dist/echarts-dashboard",
    "preview": "vite preview",
    "lint": "vue-tsc -b"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^24.10.4",
    "@vueuse/core": "^14.1.0",
    "echarts": "^6.0.0",
    "echarts-wordcloud": "^2.1.0",
    "tailwindcss": "^4.1.18",
    "vue": "^3.5.26",
    "vue-echarts": "^8.0.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/tsconfig": "^0.8.1",
    "typescript": "~5.9.3",
    "vite": "^7.3.0",
    "vue-tsc": "^3.1.8"
  }
}
